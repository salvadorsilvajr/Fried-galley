<% include partials/header%>
 
	<section class="main">
		<section class="acerca-de">
			<div class="contenedor">
				<div class="animated slideInRight foto">
					<img src="img/photo.jpg" width="180" height="180" alt="frieda S. Silva">
				</div>
				<div class="texto" >
					<h3 class="titulo">Galery</h3>
				</div>
			</div>
		</section>
	</section>	
	<div class="col-md-12">
		<div class="row text-center" style="display:flex; flex-wrap: wrap;" >
			<% pictures.forEach(function(pictures){ %>
				<div class="col-xs-12 col-sm-6 col-md-3" >
					<div class="thumbnail">
						<a href="/""<%= pictures._id%>">
							<img src="<%= pictures.image %>" alt="<%= pictures.name%>">
						</a>
						<div class="caption">
							<h3><%= pictures.name%></h3>
							<p><%= pictures.description.substring(0, 30) %>...</p>
							<p>
								<!--<a href="#" class="btn btn-primary">Comprar</a>-->
								<a href="/<%= pictures._id%>" class="btn btn-default">Detalles</a>
							</p>
						</div>	
					</div>
				</div>	
			<% }); %>	
		</div>	
	</div>	

				<p>
               <a class="btn btn-primary btn-lg" href="/new">Add a new Art-projects</a>     
            </p>
            
	

<% include partials/footer%>